gpio.mode(led2, gpio.OUTPUT)
 
gpio.mode(6,gpio.INT) function cs(dp) print(gpio.read(6)..dp)if(gpio.read(1)==0)then gpio.write(1, gpio.HIGH) else gpio.write(1, gpio.LOW)end  end gpio.trig(6, "high",cs)


