}else if(ym=="www.lwjkjdsn.com"){
	//郦湾
	document.getElementById("xm").value = "【郦湾（pc）】";
}else if(ym=="www.cshzjdc.com"){
	//舟津
	document.getElementById("xm").value = "【舟津（pc）】";
}
else if (ym=="wap.shfangzhixiao.com") { //document.write(ym)
