package tank;

public class Tank {
private int x=0,y=0,sd=5,fx=1;
private boolean tksm=true;

public boolean getTksm() {
	return tksm;
}

public void setTksm(boolean tksm) {
	this.tksm = tksm;
}

public int getSd() {
	return sd;
}

public void setSd(int sd) {
	this.sd = sd;
}

public int getFx() {
	return fx;
}

public void setFx(int fx) {
	this.fx = fx;
}

public int getX() {
	return x;
}

public void setX(int x) {
	this.x = x;
}

public int getY() {
	return y;
}

public void setY(int y) {
	this.y = y;
}
Tank(int x,int y){
	this.x=x;
	this.y=y;
}

}
