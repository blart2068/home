        $(document).ready(function (e) {
            // 图片预加载
            var completedCount = 0;
            var img_list = [];
            $("div.main_container img").each(function () {
                img_list.push(this.src);
            });
            jQuery.imgpreload(img_list, {
                each: function () {
                    //each，每次加载完一张图片后，执行此匿名函数中的代码
                    //var status = $(this).data('loaded') ? 'success' : 'error';
                    //completedCount++;
                    //var percent = Math.round((completedCount / img_list.length) * 100);
                    //console.log("当前加载了", percent, "%");
                },
                all: function () {
                    //all，当所有图片加载完毕之后，执行此函数体内的内容
                    $("div.loading_container").fadeOut(1000);
                    $("div.main_container").fadeIn(1000);
                }
            });
        });
