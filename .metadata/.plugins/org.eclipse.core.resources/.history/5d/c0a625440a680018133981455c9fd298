	<script type="text/javascript">
		function sendMail() {
			var mailForm = document.getElementById("mailForm");
			var address = "mailto:" + document.getElementById("address").value;
			mailForm.action = address;
			mailForm.submit();
		}

		function message() {
			var data = $('.info_form').serialize();
			$.ajax({
				url : '',
				data : data,
				type : 'POST',
				dataType : 'JSON',
				success : function(data) {
					if (data.status > 0) {
						alert(data.info);
					} else {
						alert("恭喜您获取房源信息");
						location.reload();
					}
				}
			})
		}
	</script>
