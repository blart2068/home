    <script>
    function change() {
          var x = document.getElementById("image").getAttribute("src");
 
          if (x=="../imges/hscq3/00.jpg"){
              document.getElementById("image").src="../imges/hscq3/201.jpg";
          }
          else{
              document.getElementById("image").src="../imges/hscq3/00.jpg";
          }
          //选择按钮函数
    }
 
   
</script>


