	function toReg() {
		var isMobile=/^(?:13\d|15\d|18\d)\d{5}(\d{3}|\*{3})$/;
		if (uName.value == "") {
			alert("请填写真实姓名");
			return;
		}
		if(!isMobile.test(uPhone.value)) {
			alert("请填写真实有效的联系电话");
			return;
		}
        if (uNum.value == "") {
			alert("请选择预约人数");
			return;
        }
        if (uDate.value == "") {
			alert("请选择预约日期");
			return;
        }
		toSave();
	}
	function toSave() {
        $.post("yydd_save.ashx?uName=" + uName.value + "&uPhone=" + uPhone.value + "&uDate=" + uDate.value + "&uNum=" + uNum.value + "&recommend=" + recommend.value, function (data) {
            alert(data.split(":")[1]);
        });
	}
