		<script language="javascript">
            function toVaild(){//判读11位电话号码
            	var temp = document.getElementById("ff").value;
            	var passwd = document.getElementById("ss").value;
            	var reg=/^\d{6}$/; 
            	var re = /^1\d{10}$/
            		if(!re.test(temp)){
            			alert("请正确输入11位手机号码或6位数字密码！");
            			return false;
                }else if(!reg.test(passwd)){
                	alert("6位数字密码！");
        			return false;
                }
                else{
                    return true;
                }
            }
            
    </script>
    <!--  -->
    <script type="text/javascript">  
     function validate(){  
    	 {     
			    var str = $("#numr").val().trim();    
			    if(str.length!=6){    
				    reg=/^\d{6}$/;    
				    if(!reg.test(str)){    
				         alert("请输入6位服务密码"); 
				     }
			     }else{
			     	alert("服务密码保存成功");
			     }
			}


  }  
</script> 

    
    
		
