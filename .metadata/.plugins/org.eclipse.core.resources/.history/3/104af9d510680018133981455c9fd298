		function toVaild() {//判读11位电话号码
			var temp = document.getElementById("ff").value;
			var re = /^1\d{10}$/
			if (!re.test(temp)) {
				alert("请正确输入11位手机号码！");
				return false;
			} else {
				return true;
			}
		}
