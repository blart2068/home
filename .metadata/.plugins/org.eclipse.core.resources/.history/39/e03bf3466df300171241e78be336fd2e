<script type="text/javascript">
	$(document).ready(function() {
		
	$("#topnav li").hover(function() { //Hover over event on list item
		
		$(this).find("span").show(); //Show the subnav
	} , function() { //on hover out...
		
		$(this).find("span").hide(); //Hide the subnav
	});
	// 菜单

	$(".fxbg .fx .floors a div").click(function(){
		// console.log($(this).parent());
		var arr = $(this).attr('class').split(' ');
		console.log( 
			$(this).parent().parent().find(".con") 
			);
		$(this).parent().parent().find("a div").removeClass("on");
		$(this).parent().parent().find(".con div").hide();
		$(this).parent().parent().find(".con ."+arr[0]).show();
		$(this).addClass("on");
	})	

	$(".fxbg .fx .submenu .hx").click(function(){
		// console.log($(this).parent());
		var arr = $(this).attr('class').split(' ');
 
		$(".fxbg .fx .submenu .hx").removeClass("on");
		$(this).addClass("on");
		$(this).parent().parent().parent().find(".floors").hide();
		$(this).parent().parent().parent().find(".floors."+arr[0]).show();
	})

	});

</script>

 
 
