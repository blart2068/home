define("mip-bsml-countdown/mip-bsml-countdown",["require","customElement","zepto"],function(t){var i=t("customElement").create(),e=t("zepto");return i.prototype.build=function(){function t(){var i=new Date(e(s).attr("data-time")),n=new Date,r=i.getTime()-n.getTime(),h=0,a=0,l=0;if(r>0){h=Math.floor(r/1e3/60/60/24),a=Math.floor(r/1e3/60/60%24),l=Math.ceil(r/1e3/60%60),h=h<10?"0"+h:h,a=a<10?"0"+a:a,l=l<10?"0"+l:l;var c=e(s).find(".count-down-box .count-down-box-sp");c[0].innerHTML=h.toString().substr(0,1),c[1].innerHTML=h.toString().substr(1,1),c[2].innerHTML=a.toString().substr(0,1),c[3].innerHTML=a.toString().substr(1,1),c[4].innerHTML=l.toString().substr(0,1),c[5].innerHTML=l.toString().substr(1,1),o=setTimeout(function(){t()},1e3)}if(r<=0)e(s).find(".count-down").hide(),clearTimeout(o)}var i=this,s=i.element,o=null;t()},i}),define("mip-bsml-countdown",["mip-bsml-countdown/mip-bsml-countdown"],function(t){return t}),function(){function t(t,i){t.registerMipElement("mip-bsml-countdown",i)}if(window.MIP)require(["mip-bsml-countdown"],function(i){t(window.MIP,i)});else require(["mip","mip-bsml-countdown"],t)}();